{
  "name": "vue-typed-form",
  "version": "0.1.0",
  "description": "Vue final-form typed integration focused on API typings",
  "keywords": [
    "vue",
    "final-form",
    "form",
    "frontend"
  ],
  "homepage": "https://github.com/CaioOliveira793/vue-typed-form",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaioOliveira793/vue-typed-form.git"
  },
  "bugs": {
    "url": "https://github.com/CaioOliveira793/vue-typed-form/issues"
  },
  "author": "Caio Oliveira <caio.vsoliveira23@gmail.com>",
  "license": "MIT",
  "type": "module",
  "source": "src/lib.ts",
  "main": "dist/lib-cjs.js",
  "module": "dist/lib-es.js",
  "types": "dist/lib-type.d.ts",
  "scripts": {
    "check:type": "tsc --noEmit",
    "check:lint": "eslint . --ignore-path .gitignore",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "test": "jest",
    "test:cov": "jest --coverage",
    "clear:build": "rm -rf dist/*",
    "clear:cache": "rm -rf .parcel-cache/*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.14.0",
    "final-form": "^4.20.9",
    "rollup": "^3.17.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "final-form": "^4.20.9",
    "vue": "^3.2.47"
  }
}
